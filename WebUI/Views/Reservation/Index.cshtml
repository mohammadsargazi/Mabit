@model List<Mabit.Models.Model.BookModel.MyBookModel>
@{
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}


@foreach (var item in Model)
{
    <div class="row pt-3 pb-3 brd-b-1">
        <div class="col-lg-4 col-sm-4 col-4 ">
            <div class="row mr-0 ml-0">
                <div class="col-12 col-lg-12 col-sm-12 pr-0 pl-0">
                    <img src="@(item.pictureUrl)"
                         class="img-fluid brd-r-15">
                </div>
            </div>
        </div>
        <div class="col-lg-6 col-sm-6 col-6 pl-0 pt-3">
            <div class="d-flex text-left ">
                <h5 class="font-weight-bolder mabit-darkGray mb-1">@(item.title)</h5>
                <span class="pl-2">
                    <i class="fa fa-star mabit-middle-gray"></i>
                    <span class="mabit-light-gray">4.8</span>
                    <span class="mabit-light-gray">/</span>
                    <span class="mabit-light-gray">5</span>
                </span>
            </div>
            <div class="d-flex text-left ">
                <span>@(item.country?.title)</span><span>,</span><span>@(item.province?.title)</span>
                <span class="pl-1">(Ads id:</span><span class="pl-1">132159</span><span>)</span>
            </div>
            <div class="mt-1">
                <a href="#" class="mabit-blue">Chat With Host</a>
            </div>
            <div class="row">
                <div class="main col-12">
                    <div class="progress">
                        <div id="p" class="progress-bar  bg-mabit-blue" role="progressbar" aria-valuenow="0"
                             aria-valuemin="0" ariavaluemax="100"></div>
                    </div>
                    <div class="block1">
                        <div id="b1" class="text-center activecirculo"></div>
                        <span class="wizard-label" localize-content>Request</span>
                    </div>
                    <div id="block2A" class="block2">
                        <div id="b2" class="text-center circulo"></div>
                        <span class="wizard-label" localize-content>Verification</span>
                    </div>
                    <div id="block3A" class="block3">
                        <div id="b3" class="text-center circulo"></div>
                        <span class="wizard-label" localize-content>Prepaymnet</span>
                    </div>
                    <div id="block4A" class="block4">
                        <div id="b4" class="text-center circulo"></div>
                        <span class="wizard-label" localize-content>Done</span>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-2 col-sm-2 col-2 pl-0 pt-3">
            <div class="text-left">
                <span class="f-size-14 mabit-darkGray">In</span>
                <span class="font-weight-bold mabit-darkGray float-right">@(item.fromDate)</span>
            </div>
            <div class="text-left">
                <span class="f-size-14 mabit-darkGray">Out</span>
                <span class="font-weight-bold mabit-darkGray float-right">@(item.toDate)</span>
            </div>
            <div class="text-left">
                <span class="f-size-14 mabit-darkGray">Cost</span>
                <span class="font-weight-bold mabit-blue float-right">@(item.cost) $</span>
            </div>
            <div class="text-center mt-30">
                <button type="button" class="btn btn-cancel">Cancel</button>
                <div class="text-center mt-2 pl-3"><a href="#" class="mabit-blue" localize-content>Similar Houses</a></div>
            </div>
        </div>
    </div>
}



<div class="row pt-3 pb-3 brd-b-1">
